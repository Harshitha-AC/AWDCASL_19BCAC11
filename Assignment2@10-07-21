import java.util.*;
class EwOw
{
public static void main(String args[])
{
String s1;int n,j=0;
Scanner sc=new Scanner(System.in);
System.out.println("enter a string");
s1=sc.next();
n=s1.length();
for(int i=0;i<n;i++)
{
j=i+1;
if(j%2!=0)
System.out.print(s1.charAt(i));
j++;
}
System.out.print(" ");

for(int i=0;i<n;i++)
{
j=i+1;
if(j%2==0)
System.out.print(s1.charAt(i));
}
}
}

