import java.util.*;
class ShellSort
{
public static void main(String args[])
{
int a[]={89,67,12,3,17};
Scanner sc=new Scanner(System.in);
int n=a.length;
int temp=0;
for(int i=0;i<n;i++)
{
for(int j=i+1;j<n;j++)
{
if(a[i]>a[j])
{
temp=a[j];
a[j]=a[i];
a[i]=temp;
}
}
}
System.out.println("after sorting");
for(int i=0;i<n;i++)
{
System.out.print(a[i]+"\t");
}
}
}
___________________________________________________________
output:
after sorting
3   12   17   67  89
